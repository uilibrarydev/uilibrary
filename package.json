{
  "homepage": "https://dinofrontend.github.io/dino_ui_library",
  "name": "dino_ui_tools",
  "version": "2.41.0",
  "description": "UI library for Dino",
  "private": false,
  "main": "index.js",
  "scripts": {
    "version": "echo $npm_package_version",
    "name": "echo $npm_package_name",
    "start": "react-scripts start",
    "build": "rollup -c",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006 public",
    "build-storybook": "storybook build",
    "lint": "eslint \"./src/**/*.{js,jsx,ts,tsx}\"",
    "prettier": "prettier --write \"./{src,stories}/**/**.{js,jsx,ts,tsx}\"",
    "check": "npm run prettier && npm run lint && tsc",
    "install-latest": "npm install --legacy-peer-deps",
    "import-all-svg-components": "node ./bin/import_all_svg_components.js",
    "publish-alpha-version": "cd \"./dist/\" && npm publish --tag alpha",
    "generate-svg-component": "node ./bin/generate_svg_component.js && npm run import-all-svg-components && npm run check"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.7",
    "@semantic-release/npm": "^12.0.1",
    "@storybook/addon-actions": "^7.6.9",
    "@storybook/addon-essentials": "^7.6.9",
    "@storybook/addon-interactions": "^7.6.9",
    "@storybook/addon-links": "^7.6.9",
    "@storybook/node-logger": "^7.6.9",
    "@storybook/react": "^7.6.10",
    "@storybook/react-webpack5": "^7.6.9",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.11.43",
    "@types/react-calendar": "^3.9.0",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-syntax-highlighter": "^15.5.6",
    "@types/react-table": "^7.7.18",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.1.1",
    "@typescript-eslint/parser": "^5.1.1",
    "ajv": "8.13.0",
    "css-loader": "^7.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.10",
    "extract-loader": "^5.1.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.1",
    "mini-css-extract-plugin": "^0.11.3",
    "postcss": "^8.1.9",
    "postcss-import": "12.0.1",
    "prettier": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "rollup": "^2.76.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-generate-package-json": "^3.2.0",
    "rollup-plugin-ignore-import": "^1.3.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-sass": "^1.12.21",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass-loader": "^10.4.1",
    "semantic-release": "^24.1.0",
    "storybook": "^7.6.14",
    "style-loader": "^3.3.4",
    "ts-loader": "^8.0.4",
    "typescript": "^4.9.5",
    "yup": "^1.2.0"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@types/react-input-mask": "^3.0.2",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "framer-motion": "4.1.17",
    "react-datepicker": "^4.25.0",
    "react-hook-form": "^7.50.0",
    "react-input-mask": "^2.0.4",
    "react-number-format": "^5.3.1",
    "react-paginate": "^8.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "react-table": "^7.8.0",
    "react-toastify": "10.0.5",
    "sass": "^1.56.1",
    "storybook-design-token": "^2.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "commit-msg": "commitlint --edit $1"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dinofrontend/dino_ui_library.git"
  }
}
